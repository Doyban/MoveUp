<?xml version='1.0' encoding='utf-8'?>
<widget id="com.doyban.moveup" version="1.1.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MoveUp</name>
    <description>Move up all the way to infinity</description>
    <author email="contact@doyban.com" href="https://doyban.com">Doyban</author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <preference name="Fullscreen" value="true" />
    <plugin name="cordova-plugin-purchase" spec="*" />
    <plugin name="cordova-plugin-facebook4" spec="*">
        <variable name="APP_ID" value="6121059601241660" />
        <variable name="APP_NAME" value="MoveUp" />
    </plugin>
    <platform name="android">
        <preference name="orientation" value="landscape" />
    </platform>
    <feature name="FacebookConnectPlugin">
        <param name="android-package" value="org.apache.cordova.facebook.ConnectPlugin" />
    </feature>
    <plugin name="cordova-plugin-x-socialsharing" spec="*" />
    <feature name="SocialSharing">
        <param name="android-package" value="nl.xservices.plugins.SocialSharing" />
    </feature>
    <!-- <plugin name="admob-plus-cordova" spec="1.28.0">
        <variable name="APP_ID_ANDROID" value="ca-app-pub-4865595196880143/6610299599" />
        <variable name="APP_ID_IOS" value="ca-app-pub-xx~yyy" />
    </plugin> -->
</widget>
